{
  "name": "backlog-notify",
  "version": "1.0.0",
  "description": "Notify commit messages to Backlog issue",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn lint:fix && ncc build ./src/main.ts",
    "lint": "eslint --ext .ts ./src/ --max-warnings 0",
    "lint:fix": "eslint --fix --ext .ts ./src/ --max-warnings 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicstone/backlog-notify/backlog-notify.git"
  },
  "keywords": [
    "Backlog",
    "Notify",
    "Project management"
  ],
  "author": "Takanori Oishi",
  "license": "MIT",
  "dependencies": {
    "axios": "0.20.0",
    "lodash": "4.17.20",
    "querystring": "0.2.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.162",
    "@types/node": "14.14.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@zeit/ncc": "0.22.1",
    "eslint": "7.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.3.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  }
}
