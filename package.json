{
  "name": "backlog-notify",
  "version": "1.0.0",
  "description": "Notify commit messages to Backlog issue",
  "main": "dist/index.js",
  "scripts": {
    "build": "prettier --write ./src/**/* && ncc build ./src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicstone/backlog-notify/backlog-notify.git"
  },
  "keywords": [
    "Backlog",
    "Notify",
    "Project management"
  ],
  "author": "Takanori Oishi",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.0",
    "@zeit/ncc": "^0.22.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build"
    }
  }
}
